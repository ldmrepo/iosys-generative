{
  "name": "@iosys/qti-editor",
  "version": "0.1.0",
  "description": "ProseMirror-based WYSIWYG editor for QTI assessment items",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@iosys/qti-core": "workspace:*",
    "@iosys/qti-ui": "workspace:*",
    "katex": "^0.16.0",
    "prosemirror-commands": "^1.6.0",
    "prosemirror-dropcursor": "^1.8.0",
    "prosemirror-gapcursor": "^1.3.0",
    "prosemirror-history": "^1.4.0",
    "prosemirror-inputrules": "^1.4.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.23.0",
    "prosemirror-state": "^1.4.0",
    "prosemirror-transform": "^1.10.0",
    "prosemirror-view": "^1.34.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/katex": "^0.16.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0"
  }
}
