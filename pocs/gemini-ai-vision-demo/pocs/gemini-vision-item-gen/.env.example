# Google AI API Key
GOOGLE_API_KEY=your_api_key_here

# Model Configuration
GEMINI_MODEL=gemini-3-flash-preview
NANO_BANANA_MODEL=gemini-3-pro-image-preview

# Output Settings
OUTPUT_DIR=./output
LOG_LEVEL=INFO

# P5 Image Generation Settings
IMAGE_RESOLUTION=1K
IMAGE_ASPECT_RATIO=1:1

# ================================================
# Cross-Validation API Keys (P4-VALIDATE)
# ================================================
# Level 2: OpenAI GPT-4o (역사/사회 기본 검증)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Level 3: Gemini Pro (민감 주제 검증)
GEMINI_PRO_MODEL=gemini-2.5-pro-preview

# Cross-Validation Settings
# - CROSS_VALIDATION_ENABLED: true/false
# - CROSS_VALIDATION_LEVEL: 1 (단일), 2 (GPT-4o 추가), 3 (Claude 추가), 4 (전문가)
CROSS_VALIDATION_ENABLED=false
CROSS_VALIDATION_LEVEL=1
